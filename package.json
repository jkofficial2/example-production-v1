        "i18next-http-backend": "1.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "11.16.9",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.8.0"
    },
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.docker",
                "preset": "iPhone 7"
            }
        }
    }
